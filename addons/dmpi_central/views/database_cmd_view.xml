<odoo>

        <!--EXECUTE MULTIPLE COMMANDS-->
        <record id="res_distributor_database_commands_view" model="ir.ui.view">
            <field name="name">Execute Database Commands</field>
            <field name="model">res.distributor.database.commands</field>
            <field name="arch" type="xml">
                <form string="Database Commands">
                    <label string="All selected journal entries will be validated and posted. You won't be able to modify them afterwards."/>
                    <footer>
                        <button string="Restart Database" name="restart_database" type="object" default_focus="1" class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_res_distributor_database_commands" model="ir.actions.act_window">
            <field name="name">Database Commands</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">res.distributor.database.commands</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="res_distributor_database_commands_view"/>
            <field name="context">{}</field>
            <field name="target">new</field>
            <field name="help">This wizard will validate all journal entries selected. Once journal entries are validated, you can not update them anymore.</field>
        </record>

        <record model="ir.values" id="execute_res_distributor_database_commands">
            <field name="model_id" ref="model_res_distributor_database" />
            <field name="name">Execute Database Commands</field>
            <field name="key2">client_action_multi</field>
            <field name="value" eval="'ir.actions.act_window,' + str(ref('action_res_distributor_database_commands'))" />
            <field name="key">action</field>
            <field name="model">res.distributor.database</field>
        </record>





    <!-- STOCK RETURN REASON -->
    <record id="action_res_distributor_database_module_form" model="ir.actions.act_window">
        <field name="name">Distributor Databases</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">res.distributor.database.module</field>
        <field name="view_type">form</field>
    </record>

    <record id="view_res_distributor_database_module_tree" model="ir.ui.view">
        <field name="name">res.distributor.database.module.tree</field>
        <field name="model">res.distributor.database.module</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree string="Stock Return Reason" editable="bottom">
                <field name="description"/>
                <field name="name"/>
                <field name="module"/>
                <field name="live"/>
                <button name="create_file" string="Create Local File" type="object" />
            </tree>
        </field>
    </record>



    <menuitem action="action_res_distributor_database_module_form" id="menu_res_distributor_database_module" parent="dmpi_menu_database_config" name="Modules" sequence="10"/>





</odoo>